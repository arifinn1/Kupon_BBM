    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport'/>

    <!-- Fonts and icons     -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet" />
    <!-- Bootstrap -->
    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Admin LTE -->
    <link href="/admin-lte/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="/admin-lte/css/skins/_all-skins.min.css" rel="stylesheet" />
    <!-- Animate -->
    <link href="/css/animate.min.css" rel="stylesheet" />
    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <!-- Costum CSS & Jquery -->
    <link href="/css/style.css" rel="stylesheet" />
    <script src="/js/script.js"></script>

    <% if (typeof datatables !== 'undefined') { %>
      <link href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css" rel="stylesheet" />
      <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <% } %>

    <% if (typeof loadingoverlay !== 'undefined') { %>
      <script src="https://cdn.jsdelivr.net/npm/gasparesganga-jquery-loading-overlay@1.6.0/src/loadingoverlay.min.js"></script>
    <% } %>


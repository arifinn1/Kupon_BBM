<aside class="main-sidebar">
  <!-- sidebar: style can be found in sidebar.less -->
  <section class="sidebar">
    <!-- Sidebar user panel -->
    <div class="user-panel">
      <div class="pull-left image">
        <img src="/admin-lte/img/user2-160x160.jpg" class="img-circle" alt="User Image">
      </div>
      <div class="pull-left info">
        <p><%= nama_user %></p>
        <a href="#">
          <i class="fa fa-circle text-success"></i> Online</a>
      </div>
    </div>
    
    <!-- sidebar menu: : style can be found in sidebar.less -->
    <ul class="sidebar-menu" data-widget="tree">

      <li class="treeview">
        <a href="#">
          <i class="fa fa-dashboard"></i>
          <span>Dashboard</span>
          <span class="pull-right-container">
            <i class="fa fa-angle-left pull-right"></i>
          </span>
        </a>
        <ul class="treeview-menu">
          <li>
            <a href="/">
              <i class="fa fa-circle-o"></i> Dashboard v1</a>
          </li>
          <li>
            <a href="/">
              <i class="fa fa-circle-o"></i> Dashboard v2</a>
          </li>
        </ul>
      </li>
      
      <li class="<%= active['tukar_kupon'] %>">
        <a href="/tukar_kupon">
          <i class="fa fa-retweet"></i>
          <span>Penukaran Kupon</span>
        </a>
      </li>

      <li class="<%= active['tukar_uang'] %>">
        <a href="/tukar_uang">
          <i class="fa fa-money"></i>
          <span>Penukaran Uang</span>
        </a>
      </li>

      <li class="<%= active['jual'] %>">
        <a href="/jual">
          <i class="fa fa-shopping-basket"></i>
          <span>Penjualan</span>
        </a>
      </li>

      <li class="<%= active['kupon'] %>">
        <a href="/kupon">
          <i class="fa fa-tags"></i>
          <span>Kupon</span>
        </a>
      </li>

      <li class="<%= active['harga'] %>">
        <a href="/harga">
          <i class="fa fa-dollar"></i>
          <span>Harga</span>
        </a>
      </li>

      <li class="<%= active['bbm'] %>">
        <a href="/bbm">
          <i class="fa fa-tint"></i>
          <span>BBM</span>
        </a>
      </li>

      <li class="<%= active['instansi'] %>">
        <a href="/instansi">
          <i class="fa fa-bank"></i>
          <span>Instansi</span>
        </a>
      </li>

      <li class="<%= active['spbu'] %>">
        <a href="/spbu">
          <i class="fa fa-industry"></i>
          <span>SPBU</span>
        </a>
      </li>

      <% if (akses_user == 'master') { %>
      <li class="<%= active['akun'] %>">
        <a href="/akun">
          <i class="fa fa-users"></i>
          <span>Akun</span>
        </a>
      </li>

      <li class="<%= active['seting'] %>">
        <a href="/seting">
          <i class="fa fa-sliders"></i>
          <span>Seting</span>
        </a>
      </li>
      <% } %>

    </ul>
  </section>
  <!-- /.sidebar -->
</aside>